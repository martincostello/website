@model string
@{
    string? canonical = Model;
    SiteOptions options = Options.Value;
}
<link rel="canonical" href="@canonical" />
<link rel="manifest" href="@Url.Content("~/manifest.webmanifest")" asp-append-version="true" />
<link rel="sitemap" type="application/xml" href="@Url.Content("~/sitemap.xml")" asp-append-version="true" />
<link rel="shortcut icon" type="image/x-icon" href="@Url.CdnContent("~/favicon.ico", options)" asp-append-version="true" />
<link rel="apple-touch-icon" sizes="57x57" href="@Url.CdnContent("~/apple-touch-icon-57x57.png", options)" asp-append-version="true" />
<link rel="apple-touch-icon" sizes="60x60" href="@Url.CdnContent("~/apple-touch-icon-60x60.png", options)" asp-append-version="true" />
<link rel="apple-touch-icon" sizes="72x72" href="@Url.CdnContent("~/apple-touch-icon-72x72.png", options)" asp-append-version="true" />
<link rel="apple-touch-icon" sizes="76x76" href="@Url.CdnContent("~/apple-touch-icon-76x76.png", options)" asp-append-version="true" />
<link rel="apple-touch-icon" sizes="114x114" href="@Url.CdnContent("~/apple-touch-icon-114x114.png", options)" asp-append-version="true" />
<link rel="apple-touch-icon" sizes="120x120" href="@Url.CdnContent("~/apple-touch-icon-120x120.png", options)" asp-append-version="true" />
<link rel="apple-touch-icon" sizes="144x144" href="@Url.CdnContent("~/apple-touch-icon-144x144.png", options)" asp-append-version="true" />
<link rel="apple-touch-icon" sizes="152x152" href="@Url.CdnContent("~/apple-touch-icon-152x152.png", options)" asp-append-version="true" />
<link rel="apple-touch-icon" sizes="180x180" href="@Url.CdnContent("~/apple-touch-icon-180x180.png", options)" asp-append-version="true" />
<link rel="icon" type="image/png" href="@Url.CdnContent("~/android-chrome-192x192.png", options)" sizes="192x192" asp-append-version="true" />
<link rel="icon" type="image/png" href="@Url.CdnContent("~/favicon-32x32.png", options)" sizes="32x32" asp-append-version="true" />
<link rel="icon" type="image/png" href="@Url.CdnContent("~/favicon-96x96.png", options)" sizes="96x96" asp-append-version="true" />
<link rel="icon" type="image/png" href="@Url.CdnContent("~/favicon-16x16.png", options)" sizes="16x16" asp-append-version="true" />
<link rel="dns-prefetch" href="https://@options?.ExternalLinks?.Cdn?.Host" />
<link rel="dns-prefetch" href="https://cdnjs.cloudflare.com">
<link rel="dns-prefetch" href="https://fonts.googleapis.com">
<link rel="dns-prefetch" href="https://fonts.gstatic.com">
<link rel="dns-prefetch" href="https://www.googletagmanager.com">
<link rel="preconnect" href="https://@options?.ExternalLinks?.Cdn?.Host" />
<link rel="preconnect" href="https://cdnjs.cloudflare.com">
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com">
<link rel="preconnect" href="https://www.googletagmanager.com">
