{
  "name": "martincostellocom",
  "private": true,
  "version": "3.0.0",
  "description": "Martin Costello's website",
  "scripts": {
    "build": "npm run compile && npm run format && npm run lint",
    "compile": "webpack",
    "format": "prettier --write assets/**/*.ts && stylelint --fix assets/*/*.css",
    "format-check": "prettier --check assets/**/*.ts && stylelint assets/*/*.css",
    "lint": "eslint --ext .ts assets",
    "test": "jest --coverage",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/martincostello/website.git"
  },
  "author": "martincostello",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.2.3",
    "jest": "^28.1.3",
    "mini-css-extract-plugin": "^2.7.6",
    "moment": "^2.29.4",
    "prettier": "^3.0.1",
    "style-loader": "^3.3.3",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "ts-loader": "^9.4.4",
    "ts-jest": "^28.0.8",
    "tsify": "^5.0.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-remove-empty-scripts": "^1.0.3"
  }
}
